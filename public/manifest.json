{
  "manifest_version": 3,
  "name": "UTN.BA Helper (ex Siga Helper)",
  "short_name": "UTN.BA Helper",
  "version": "0.0.0",
  "description": "UTN.BA Helper facilita el uso de la web de la UTN - FRBA.",
  "author": "Pablo Mat√≠as Gomez",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.guarani.frba.utn.edu.ar/*"
      ],
      "js": [
        "guarani/main.js"
      ],
      "css": [
        "guarani/main.css"
      ]
    },
    {
      "matches": [
        "*://*.kolla.frba.utn.edu.ar/*"
      ],
      "js": [
        "guarani/main-kolla.js"
      ],
      "css": [
        "guarani/main.css"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "guarani/foreground.js"
      ],
      "matches": [
        "*://*.frba.utn.edu.ar/*"
      ],
      "use_dynamic_url": true
    },
    {
      "resources": [
        "guarani/*.js.map"
      ],
      "matches": [
        "*://*.frba.utn.edu.ar/*"
      ]
    }
  ],
  "host_permissions": [
    "*://*.kolla.frba.utn.edu.ar/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ]
}